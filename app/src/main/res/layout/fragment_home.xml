<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.contactos.FragmentContactos">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_lista_contactos"
        android:layout_width="317dp"
        android:layout_height="267dp"
        android:layout_marginTop="84dp"
        android:background="#FFFFFF"
        android:elevation="3dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:clickable="true"
        android:src="@drawable/ic_baseline_person_add_24"
        app:backgroundTint="#8BC34A"
        app:layout_constraintBottom_toTopOf="@+id/rv_lista_contactos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.867"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/rastreo"
        android:layout_width="119dp"
        android:layout_height="74dp"
        android:layout_marginTop="32dp"
        android:background="#CFCFCF"
        android:elevation="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rv_lista_contactos"
        app:srcCompat="@drawable/logo_5"
        app:tint="#000000"
        app:tintMode="multiply" />

</androidx.constraintlayout.widget.ConstraintLayout>